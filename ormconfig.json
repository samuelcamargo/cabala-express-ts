{
  "type": "sqlite",
  "database": "./db/database.sqlite",
  "synchronize": true,
  "logging": false,
  "entities": [
    "src/entities/*.ts"
  ],
  "migrations": [
    "src/migrations/**/*.ts"
  ],
  "subscribers": [
    "src/subscribers/**/*.ts"
  ],
  "cli": {
    "entitiesDir": "src/entities",
    "migrationsDir": "src/migrations",
    "subscribersDir": "src/subscribers"
  }
}